extend type Query {
  getSubDepartmentFilters(department: String!, subDepartment: String!): Filters!
}

type Filters {
  Price: PriceRange
  Rating: Int
  Companies: [String]
  CompanyCertifications: [String]
  ProductCertifications: [String]
}

type PriceRange {
  min: Float
  max: Float
}

enum Department {
  HomeGarden
  ClothingAccessories
  HealthBathBeauty
  ToysKidsBabies
}